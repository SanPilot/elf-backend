{
  "moduleName": "Index.js (index.js)",
  "usePort": 3000,
  "freqBlock": {
    "messagesAllowedPerSecond": 5,
    "blockTime": 10000
  },
  "connectionDelay": 2000,
  "requireModules": {
    "users": "users.js",
    "tasks": "tasks.js",
    "notify": "notify.daemon.js",
    "fileStorage": "fileStorage.js"
  },
  "apiRoutes": {
    "getUsers": ["users", "getUsers"],
    "auth": ["users", "auth"],
    "createUser": ["users", "createUser"],
    "removeUser": ["users", "removeUser"],
    "addTask": ["tasks", "addTask"],
    "listTasks": ["tasks", "listTasks"],
    "modifyTask": ["tasks", "modifyTask"],
    "getNotifications": ["notify", "getNotifications"]
  }
}
