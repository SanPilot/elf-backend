{
  "moduleName": "Index.js (index.js)",
  "usePort": 3000,
  "maxMessageSize": 1000000,
  "freqBlock": {
    "messagesAllowedPerSecond": 5,
    "blockTime": 10000
  },
  "connectionDelay": 2000,
  "requireModules": {
    "users": "users.js",
    "tasks": "tasks.js",
    "notify": "notify.daemon.js",
    "fileStorage": "fileStorage.js",
    "projects": "projects.js",
    "shortCodes": "shortCodes.js"
  },
  "apiRoutes": {
    "getUsers": ["users", "getUsers"],
    "auth": ["users", "auth"],
    "createUser": ["users", "createUser"],
    "modifyUser": ["users", "modifyUser"],
    "removeUser": ["users", "removeUser"],
    "addTask": ["tasks", "addTask"],
    "listTasks": ["tasks", "listTasks"],
    "modifyTask": ["tasks", "modifyTask"],
    "addComment": ["tasks", "addComment"],
    "modifyComment": ["tasks", "modifyComment"],
    "getNotifications": ["notify", "getNotifications"],
    "createUpload": ["fileStorage", "createUpload"],
    "finalizeUpload": ["fileStorage", "finalizeUpload"],
    "fileInfo": ["fileStorage", "fileInfo"],
    "createDownload": ["fileStorage", "createDownload"],
    "createProject": ["projects", "createProject"],
    "modifyProject": ["projects", "modifyProject"],
    "listProjects": ["projects", "listProjects"],
    "listProjectItems": ["projects", "listProjectItems"],
    "resolveShortCode": ["shortCodes", "resolveShortCode"]
  },
  "specialConnections": {
    "fileupload": ["fileStorage", "upload"],
    "filedownload": ["fileStorage", "download"]
  }
}
